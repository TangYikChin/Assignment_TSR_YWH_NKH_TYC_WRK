<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClothedActivity">

    <TextView
        android:id="@+id/clothed"
        android:layout_width="414dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/clothed"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/clothed"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/clothed" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="152dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView6"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="0dp"
        android:text="@string/clothed1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="152dp"
        android:layout_height="29dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="0dp"
        android:text="@string/price_clothed" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/clothed"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clothed"
        app:srcCompat="@drawable/clothed1" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="152dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView7"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="32dp"
        android:text="@string/clothed2" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="152dp"
        android:layout_height="27dp"
        android:layout_below="@+id/textView12"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="32dp"
        android:text="@string/price_clothed2" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/textView11"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/clothed2" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_below="@+id/imageView12"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="0dp"
        android:text="@string/clothed3" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="150dp"
        android:layout_height="29dp"
        android:layout_below="@+id/textView20"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="0dp"
        android:text="@string/price_clothed3" />

</RelativeLayout>